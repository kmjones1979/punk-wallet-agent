# Add your Infura ID here for better wallet connection
NEXT_PUBLIC_INFURA_ID=your_infura_id_here


# You can get a free Infura ID from https://infura.io/
# Replace 'your_infura_id_here' with your actual Infura ID
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# AI_PROVIDER=google
